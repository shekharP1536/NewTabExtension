{
   "manifest_version": 3,
   "name": "New Tab",
   "version": "3.0.1",
   "author": "Shekhar",
   "permissions": ["storage"],
   "icons": {
       "16": "/assets/icon16.png",
       "48": "/assets/icon48.png",
       "128": "/assets/icon128.png"
   },
   "action": {
       "default_popup": "popup.html",
       "default_icon": "/assets/ok.png"
   },
   "description": "Cool customized New Tab must try once with Todo, Clock , Wallpaper , WorldNews",
   "chrome_url_overrides": {
       "newtab": "newtab.html"
   }
}